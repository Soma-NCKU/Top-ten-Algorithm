# Using while loop

pos = -1

def search(list, n):
    i = 0
    while i < len(list):
        if list[i] == n:
            globals()['pos'] = i
            return True
        i = i+1
    return False

list = [5, 10, 20 , 2, 9]
n = 9

if search(list, n):
    print("Found at {}".fomate(pos+1))
else:
    print("Not Found")
